@model Ogretmen

<div class="row">
    <div class="col-8">
        <h2>Ogretmen Ekle</h2>
        <hr>
        <form asp-controller="Ogretmen" asp-action="Edit" method="post">
            <input type="hidden" asp-for="OgretmenId">
            <div class="mb-3">
                <label asp-for="OgretmenAd" class="form-label">Ogretmen Ad</label>
                <input asp-for="OgretmenAd" class="form-control">
            </div>
            <div class="mb-3">
                <label asp-for="OgretmenSoyad" class="form-label">Ogretmen Soyad </label>
                <input asp-for="OgretmenSoyad" class="form-control">
            </div>
            <div class="mb-3">
                <label asp-for="Eposta" class="form-label">Eposta</label>
                <input asp-for="Eposta" class="form-control">
            </div>
            <div class="mb-3">
                <label asp-for="Telefon" class="form-label">Telefon</label>
                <input asp-for="Telefon" class="form-control">
            </div>
            <div class="mb-3">
                <label asp-for="BaslamaTarihi" class="form-label">Telefon</label>
                <input asp-for="BaslamaTarihi" class="form-control">
            </div>
            <button type="submit" class="btn btn-secondary">Kaydet</button>
        </form>
    </div>
</div>